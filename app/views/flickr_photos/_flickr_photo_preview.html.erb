
<div id="<%= dom_id flickr_photo %>">

  <div class='container'>
      <% if flickr_photo.photo_set_count > 0 %>


        <h5>album</h5>

        <%= link_to '...', flickr_photo_path(flickr_photo), class:'triple-dot-link' %>

      <% else %>
        <strong>embed:</strong>
        <%= flickr_photo.embed.first(4) %>
      <% end %>
  </div>
</div>
