<div id="<%= dom_id instrument %>" class="instrument-preview">

  <% if instrument.make.present? %>
  <p class="make-link">
  <%= instrument.make.title %>
  </p>
  <% end %>

  <% if instrument.flickr_photo.present? %>
    <%= instrument.flickr_photo.photo_set_count %>
  <% end %>

  <p>
    <%= instrument.year_or_year_est %>

    <%= instrument.manafacturer %>

    <%= instrument.model_type %>
  </p>


</div>
