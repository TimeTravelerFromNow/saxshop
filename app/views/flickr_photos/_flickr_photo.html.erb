
<% photo_set ||= nil  %>
<div id="<%= dom_id flickr_photo %>">

  <div class='container'>
      <% if flickr_photo.photo_set.present? %>
      <% flickr_photo.photo_set.each do |fp| %>

          <%= image_tag fp.source %>

      <% end %>
      <% else %>

        <%= sanitize flickr_photo.embed %>
      <% end %>
  </div>
</div>
